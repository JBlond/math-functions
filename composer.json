{
  "name": "jblond/math-functions",
  "type": "library",
  "description": "description",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Mario",
      "email": "leet31337@web.de"
    }
  ],
  "require": {
    "php": ">= 7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7 || ^8 || ^9 || ^10 || ^11",
    "vimeo/psalm": "^5.12"
  },
  "autoload": {
    "psr-4": {
      "jblond\\math\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "jblond\\": "tests/"
    }
  },
  "config": {
    "classmap-authoritative": true
  },
  "scripts": {
    "coverage": [
      "@putenv XDEBUG_MODE=coverage",
      "phpunit --coverage-html doc"
    ],
    "phpunit": "phpunit ./tests/",
    "test": "phpunit --colors=always --testdox"
  }
}
